$GetSiteUrl = "SiteUrl"
$GetListName ="ListName"
Connect-PnPOnline -Url $GetSiteUrl -CurrentCredentials
$listItems = Get-PnPListItem -List $GetListName -Fields "Title","Fields"
$listItems | %{new-object PSObject -Property @{Title=$_["Title"];Description=$_["Description"]}} | select Title,Description
#The above line is used to view all the selected items or else you can run foreach to capture every item
$listItems
